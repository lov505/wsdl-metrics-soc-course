  <wsdl:portType name="WSMetalMakerSoap">
    <wsdl:operation name="SetInstrumentalProbability">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sets the instrumental probability. Value must be between 0 and 100.</wsdl:documentation>
      <wsdl:input message="tns:SetInstrumentalProbabilitySoapIn" />
      <wsdl:output message="tns:SetInstrumentalProbabilitySoapOut" />
    </wsdl:operation>
    <wsdl:operation name="SetAlbumTitleAsSongTitleProbability">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sets the probability for album title occurring also as song title. This can happen only once in an album. Value must be between 0 and 100. This method has no effect if the request type is set to Song.</wsdl:documentation>
      <wsdl:input message="tns:SetAlbumTitleAsSongTitleProbabilitySoapIn" />
      <wsdl:output message="tns:SetAlbumTitleAsSongTitleProbabilitySoapOut" />
    </wsdl:operation>
    <wsdl:operation name="SetTenseAlterationProbability">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sets the probability for tense alteration between verses. Value must be between 0 and 100. This method has no effect if the tense is set to something else than Undefined.</wsdl:documentation>
      <wsdl:input message="tns:SetTenseAlterationProbabilitySoapIn" />
      <wsdl:output message="tns:SetTenseAlterationProbabilitySoapOut" />
    </wsdl:operation>
    <wsdl:operation name="SetTense">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sets the tense to be used in album or song generation.</wsdl:documentation>
      <wsdl:input message="tns:SetTenseSoapIn" />
      <wsdl:output message="tns:SetTenseSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="SetRequestType">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sets the request type, which determines whether to generate an album or a song.</wsdl:documentation>
      <wsdl:input message="tns:SetRequestTypeSoapIn" />
      <wsdl:output message="tns:SetRequestTypeSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="SetTitle">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sets the title to be used in album or song generation.</wsdl:documentation>
      <wsdl:input message="tns:SetTitleSoapIn" />
      <wsdl:output message="tns:SetTitleSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="SetSongCount">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sets the song count title to be used in album generation. This method has no effect if the request type is set to Song. Value must be between 0 and 100. If the value is set to zero, a random amount of songs will be created.</wsdl:documentation>
      <wsdl:input message="tns:SetSongCountSoapIn" />
      <wsdl:output message="tns:SetSongCountSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="UseBuiltInData">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Enables or disables built in vocabulary and structure data. Default is true.</wsdl:documentation>
      <wsdl:input message="tns:UseBuiltInDataSoapIn" />
      <wsdl:output message="tns:UseBuiltInDataSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="AddUserData">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Feeds Metal Maker new xml data. The data must comply to Metal Maker XML Data 3.0 specification available at &lt;a target=_blank href="http://metalmaker.net"&gt;metalmaker.net&lt;/a&gt;.</wsdl:documentation>
      <wsdl:input message="tns:AddUserDataSoapIn" />
      <wsdl:output message="tns:AddUserDataSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="ClearSettings">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Clears all settings: tense, request type, title and user-provided vocabulary.</wsdl:documentation>
      <wsdl:input message="tns:ClearSettingsSoapIn" />
      <wsdl:output message="tns:ClearSettingsSoapOut" />
    </wsdl:operation>
    <wsdl:operation name="Generate">
      <wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Generates an album or song depending on the parameters given. The default is Album.</wsdl:documentation>
      <wsdl:input message="tns:GenerateSoapIn" />
      <wsdl:output message="tns:GenerateSoapOut" />
    </wsdl:operation>
  </wsdl:portType>
